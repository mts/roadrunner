{
  "testRegex": ".test\\.js$",
  "roots": ["<rootDir>/src"],
  "transform": {
    "^.+\\.js?$": "babel-jest"
  },
  "collectCoverageFrom": ["packages/**/*.js", "packages/**/*.jsx"],
  "coveragePathIgnorePatterns": ["node_modules", ".test.js"],
  "setupFilesAfterEnv": ["<rootDir>/jest.config.js"],
  "moduleNameMapper": {
    "\\.(css|less|scss)$": "identity-obj-proxy"
  },
  "moduleFileExtensions": ["js"]
}
