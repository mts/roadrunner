{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "react-hooks", "jsx-a11y", "jest", "prettier"],
  "extends": [
    "eslint:recommended",
    "airbnb",
    "airbnb/hooks",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:jest/recommended",
    "prettier",
    "prettier/react"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "commonjs": true,
    "mocha": true,
    "jasmine": true,
    "jest": true,
    "amd": true
  },
  "globals": {
    "jsdom": true,
    "document": false,
    "expect": false,
    "jest": false,
    "renderShallow": false,
    "renderMount": false
  },
   "settings": {
    "react": {
      "version": "16.10.2"
    }
  },
  "rules": {
    "no-prototype-builtins": 0,
    "no-unused-vars": 0
  }
}
